{
  "name": "slidev-theme-gemini-workspace",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter demo dev",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "format": "pnpm -r format",
    "lint": "pnpm -r lint",
    "prepare": "husky",
    "docs:dev": "pnpm --filter docs dev"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "pnpm -r lint",
      "pnpm -r format"
    ],
    "**/*.{json,yaml,yml}": [
      "pnpm -r format"
    ]
  }
}

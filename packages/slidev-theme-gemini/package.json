{
  "name": "slidev-theme-gemini",
  "version": "0.1.0",
  "description": "Gemini theme for Slidev",
  "keywords": [
    "slidev",
    "slidev-theme"
  ],
  "license": "MIT",
  "author": "",
  "scripts": {
    "format": "oxfmt --write",
    "lint": "oxlint"
  },
  "dependencies": {
    "@slidev/types": "^52.11.3",
    "@vueuse/motion": "^3.0.3"
  },
  "devDependencies": {
    "oxfmt": "catalog:",
    "oxlint": "catalog:"
  },
  "peerDependencies": {
    "@slidev/client": ">=0.47.0",
    "vue": ">=3.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.47.0"
  },
  "slidev": {
    "defaults": {
      "fonts": {
        "sans": "Space Grotesk",
        "serif": "Space Grotesk",
        "mono": "IBM Plex Mono",
        "italic": true,
        "weights": "300,400,500,600,700"
      }
    }
  }
